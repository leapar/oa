<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<window xmlns="http://schemas.haulmont.com/cuba/window.xsd"
        class="com.purvar.ito.oa.web.extuser.ExtUserEdit"
        extends="/com/haulmont/cuba/gui/app/security/user/edit/user-edit.xml"
        messagesPack="com.purvar.ito.oa.web.extuser"
        xmlns:ext="http://schemas.haulmont.com/cuba/window-ext.xsd">
    <layout>
        <groupBox id="propertiesBox">
            <grid id="propertiesGrid">
                <rows>
                    <row id="propertiesRow">
                        <fieldGroup id="fieldGroupLeft"
                                    width="350px">
                            <column width="200px">
                                <field id="login"
                                       caption="账号"/>
                                <field id="firstName"
                                       visible="false"/>
                                <field id="middleName"
                                       visible="false"/>
                                <field id="lastName"
                                       visible="false"/>
                                <field id="name"
                                       caption="姓名"/>
                                <field caption="区域"
                                       ext:index="7"
                                       property="area"/>
                                <field id="email"
                                       visible="false"/>
                                <field caption="公司"
                                       property="company"/>
                                <field caption="状态"
                                       property="status"/>
                                <field caption="类型"
                                       property="type"/>
                                <field editable="false"
                                       property="device_no"
                                       visible="false"/>
                            </column>
                        </fieldGroup>
                        <fieldGroup id="fieldGroupRight">
                            <column width="100px">
                                <field id="group"
                                       editable="false"
                                       visible="false"/>
                                <field id="position"
                                       custom="true"
                                       editable="false"
                                       visible="false"/>
                                <field id="language"
                                       editable="false"
                                       visible="false"/>
                                <field id="timeZone"
                                       editable="false"
                                       visible="false"/>
                                <field id="ipMask"
                                       editable="false"
                                       visible="false"/>
                            </column>
                        </fieldGroup>
                    </row>
                </rows>
            </grid>
        </groupBox>
        <split id="split">
            <vbox id="substPanel"
                  visible="false"/>
        </split>
    </layout>
</window>
